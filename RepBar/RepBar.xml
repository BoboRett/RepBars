<Ui xmlns="http://www.blizzard.com/wow/ui/" 
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
xsi:schemaLocation="http://www.blizzard.com/wow/ui/ 
..\..\FrameXML\UI.xsd"> 

<Frame name="RepBarFrame">
 <Scripts>
  <OnLoad>
   RepBar_OnLoad(self);
  </OnLoad>
  <OnEvent>
   RepBar_OnEvent(self, event, ...);
  </OnEvent>
  <OnUpdate>
   RepBar_OnUpdate(self, elapsed);
  </OnUpdate>
 </Scripts>
</Frame>

	<Button name="RepBarButton" frameStrata="BACKGROUND" toplevel="true" movable="true" parent="UIParent" virtual="true">
		<Size>
			<AbsDimension x="5" y="5"/>
		</Size>
		<Scripts>
			<OnUpdate>
				RepBar_OnUpdate(arg1);
			</OnUpdate>
		</Scripts>
	</Button>

</Ui> 
