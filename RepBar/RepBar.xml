<Ui xmlns="http://www.blizzard.com/wow/ui/"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\..\FrameXML\UI.xsd">
	<Script file="RepBar.lua"/>

	<Frame name="RepBarLoader">
		<Scripts>
			<OnLoad>
				RepBar_OnLoad(self);
			</OnLoad>
			<OnEvent>
				RepBar_OnEvent(self, event, ...);
			</OnEvent>
			<OnUpdate>
				RepBar_OnUpdate(self, elapsed);
			</OnUpdate>
		</Scripts>
	</Frame>

</Ui>
